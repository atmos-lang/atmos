val t = @{ x=1 }
print(t['x'])       ;; --> 1
print(t[:x])        ;; --> 1
print(t.x)          ;; --> 1
print(t.y)          ;; --> nil

val v = #{ 1 }
set v[0] = 10       ;; #{ 10 }
set v[#v] = 20      ;; #{ 10, 20 }
print(v[0])         ;; --> 10
print(v[#v-1])      ;; --> 20
print(v[#v])        ;; ERR: out of bounds
